<?xml version="1.0"?>
<configuration>
  <system.web>
    <compilation batch="false" />
  </system.web>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="server/start.js" verb="*" modules="iisnode" />
    </handlers>
    <rewrite>
      <rules>
        <rule name="battlecruiser">
          <match url="/*" />
          <action type="Rewrite" url="server/start.js" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
